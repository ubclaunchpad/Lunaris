{
    "name": "@lunaris/frontend",
    "version": "0.1.0",
    "private": true,
    "description": "Next.js frontend application for the Lunaris cloud gaming platform",
    "scripts": {
        "dev": "next dev --turbopack --experimental-https",
        "dev:https": "next dev --turbopack --experimental-https",
        "build": "next build --turbopack",
        "start": "next start",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "type-check": "tsc --noEmit",
        "clean": "rm -rf .next out",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "test:coverage": "vitest --coverage",
        "setup:dcv": "node scripts/setup-dcv-sdk.js"
    },
    "dependencies": {
        "@cloudscape-design/components": "^3.0.1145",
        "@cloudscape-design/design-tokens": "^3.0.64",
        "@cloudscape-design/global-styles": "^1.0.49",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-separator": "^1.1.7",
        "@radix-ui/react-slot": "^1.2.3",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "lucide-react": "^0.544.0",
        "next": "15.5.4",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "tailwind-merge": "^3.3.1"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@vitest/coverage-v8": "^2.1.0",
        "eslint": "^9",
        "eslint-config-next": "15.5.4",
        "lightningcss": "^1.30.2",
        "mkcert": "^3.2.0",
        "tailwindcss": "^4",
        "tw-animate-css": "^1.4.0",
        "typescript": "5.9.3",
        "vitest": "^2.1.0"
    }
}
